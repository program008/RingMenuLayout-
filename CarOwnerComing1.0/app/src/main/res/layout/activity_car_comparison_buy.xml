<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/baoxian1"
            android:scaleType="fitXY"/>

        <View style="@style/public_gray_horizontal_line3" />
        <RelativeLayout
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="保险公司一"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:layout_centerVertical="true"/>
        </RelativeLayout>

    </LinearLayout>

    <View style="@style/public_gray_horizontal_line" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/baoxian2"
            android:scaleType="fitXY"/>

        <View style="@style/public_gray_horizontal_line3" />
        <RelativeLayout
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="保险公司二"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:layout_centerVertical="true"/>
        </RelativeLayout>

    </LinearLayout>

    <View style="@style/public_gray_horizontal_line" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/baoxian3"
            android:scaleType="fitXY"/>

        <View style="@style/public_gray_horizontal_line3" />
        <RelativeLayout
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="保险公司三"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:layout_centerVertical="true"/>
        </RelativeLayout>

    </LinearLayout>

    <View style="@style/public_gray_horizontal_line" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/baoxian4"
            android:scaleType="fitXY"/>

        <View style="@style/public_gray_horizontal_line3" />
        <RelativeLayout
            android:layout_weight="5"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="保险公司四"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:layout_centerVertical="true"/>
        </RelativeLayout>

    </LinearLayout>

    <View style="@style/public_gray_horizontal_line" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/baoxian5"
